<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
<display-name>Test</display-name>
<welcome-file-list>
 <welcome-file>test.html</welcome-file>
 <welcome-file>testjsp.jsp</welcome-file>
</welcome-file-list>
<servlet>
    <servlet-name>ServletNoSession</servlet-name>
    <servlet-class>com.test.session.ServletNoSession</servlet-class>
</servlet>
<servlet-mapping>
    <servlet-name>ServletNoSession</servlet-name>
    <url-pattern>/ServletNoSession</url-pattern>
</servlet-mapping>
<servlet>
    <servlet-name>ServletwithSession</servlet-name>
    <servlet-class>com.test.session.ServletwithSession</servlet-class>
</servlet>
<servlet-mapping>
    <servlet-name>ServletwithSession</servlet-name>
    <url-pattern>/ServletwithSession</url-pattern>
</servlet-mapping>
</web-app>